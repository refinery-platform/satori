{
  "env": {
    "browser": true
  },
  "globals": {
    "$": false,
    "require": false,
    "process": false,
    "window": false
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "brace-style": [2, "1tbs"],
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "key-spacing": 0,
    "no-console": 1,
    "no-undef": 2,
    "no-use-before-define": 0,
    "object-curly-spacing": [2, "never"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "wrap-iife": 2
  }
}
